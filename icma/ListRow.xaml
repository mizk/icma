<UserControl x:Class="icma.ListRow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:icma"
             mc:Ignorable="d"
             Width="1150"
             Height="300"
             Loaded="UserControl_Loaded"
             Unloaded="UserControl_Unloaded">
    <StackPanel Orientation="Horizontal" Height="300">
        <local:ListCell x:Name="Cell1" />
        <local:ListCell x:Name="Cell2"/>
        <local:ListCell x:Name="Cell3"/>
        <local:ListCell x:Name="Cell4"/>
        <local:ListCell x:Name="Cell5"/>
    </StackPanel>

</UserControl>
